      "Edit(*.config.ts)",
      "Bash(npm install)",
      "Bash(npm install:*)",
      "Bash(npm run dev)",
      "Bash(npm run build)",
      "Bash(npm run lint)",
      "Bash(npm run type-check)",
      "Bash(npm run test)",
      "Bash(npm list)",
      "Bash(npm outdated)",
      "Bash(npm publish)",
      "Bash(node --version)",
      "Bash(npm --version)",
      "Bash(ls)",
      "Bash(ls:*)",
      "Bash(pwd)",
